{
  "name": "mm",
  "version": "1.0.0",
  "scripts": {
    "test": "vitest",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "test:watch": "vitest --watch",
    "format": "prettier --check .",
    "format:write": "prettier --write .",
    "infra:up": "docker compose up -d",
    "infra:down": "docker compose down",
    "infra:logs": "docker compose logs -f",
    "stack:up": "docker compose -f docker-compose.stack.yml up -d --build",
    "stack:down": "docker compose -f docker-compose.stack.yml down",
    "stack:logs": "docker compose -f docker-compose.stack.yml logs -f",
    "infra:wait": "wait-on tcp:127.0.0.1:5432 tcp:127.0.0.1:6379",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "dev:full": "npm run infra:up && npm run infra:wait && npm run db:generate && npm run db:migrate:deploy && npm run dev",
    "worker:llm": "node backend/workers/llm-parse-worker.js",
    "worker:drafts": "node backend/workers/order-draft-timeout-worker.js"
  },
